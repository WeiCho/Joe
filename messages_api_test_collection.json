        ],
        "body": {
          "mode": "raw",
          "raw": "{\n    \"name\": \"AA AA\",\n    \"subject\": \"return\",\n    \"message\": \"Quality is not an act, it is a habit. Strive for excellence every day!\",\n    \"email\": \"AAAA@gmail.com\"\n}"
        },
        "url": {
          "raw": "https://api.practicesoftwaretesting.com/messages",
          "protocol": "https",
          "host": [
            "api",
            "practicesoftwaretesting",
            "com"
          ],
          "path": [
            "messages"
          ]
        }
      },
      "response": [],
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "if (pm.response.status === 'OK') {",
              "    var jsonData = pm.response.json();",
              "    pm.test(\"Response contains ID\", function () {",
              "        pm.expect(jsonData.id).to.be.a('string');",
              "    });",
              "} else {",
              "    pm.test(\"Response should not contain ID\", function () {",
              "        pm.expect(pm.response.text()).to.not.include('id');",
              "    });",
              "}"
            ],
            "type": "text/javascript"
          }
        }
      ]
    },
